{
    "name": "masterro/laravel-access-screen",
    "description": "Provides a configurable password screen for your Laravel application.",
    "type": "library",
    "authors": [
        {
            "name": "Roman Ihoshyn",
            "email": "igoshin18@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0",
        "illuminate/view": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "MasterRO\\AccessScreen\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MasterRO\\AccessScreen\\AccessScreenServiceProvider"
            ]
        }
    }
}
